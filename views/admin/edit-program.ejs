<% include ../partials/header %>
<% include ../partials/admin-header %>
<div class="row" style="margin-left: 50px; margin-right: 50px">


<form action="/admin/program/edit/<%= items[0].programId%>" method="post" class="col s12">
<input type="hidden" name="programId" value="<%=items[0].programId%>">
<div class="form-group row">
    <label for="programCode" class="col-2 col-form-label">Program Code</label>
    <div class="col-10">
        <input type="text" name="programCode" value="<%=items[0].programCode%>" class="form-control" maxlength = "255">
    </div>
</div>
<div class="form-group row">
        <label for="programName" class="col-2 col-form-label">Program Name</label>
        <div class="col-10">
            <input type="text" name="programName" value="<%=items[0].programName%>" class="form-control" maxlength = "255">
        </div>
</div>
<input type="hidden" name="schoolId" value="<%=items[0].schoolId%>">
<div class="form-group row">
        <label for="schoolName" class="col-2 col-form-label">School Name</label>
        <div class="col-10">
            <input type="text" name="schoolName" value="<%=items[0].schoolName%>" class="form-control" maxlength = "255" readonly>
        </div>
</div>
<div class="form-group row">
        <label for="programLength" class="col-2 col-form-label">Program Length</label>
        <div class="col-10">
            <input type="text" name="programLength" value="<%=items[0].programLength%>" class="form-control" maxlength = "255">
        </div>
</div>
<div class="form-group row">
        <label for="tuitionDomestic" class="col-2 col-form-label">Domestic Tuition</label>
        <div class="col-10">
            <input type="number" name="tuitionDomestic" value="<%=items[0].tuitionDomestic%>" class="form-control" maxlength = "10">
        </div>
</div>
<div class="form-group row">
        <label for="tuitionInternational" class="col-2 col-form-label">International Tuition</label>
        <div class="col-10">
            <input type="number" name="tuitionInternational" value="<%=items[0].tuitionInternational%>" class="form-control" maxlength = "10">
        </div>
</div>
<div class="form-group row">
        <label for="isCoOp" class="col-2 col-form-label">Is Co-Op (1 or 0)</label>
        <div class="col-10">
            <input type="number" name="isCoOp" value="<%=items[0].isCoOp%>" class="form-control" min="0" max="1" maxlength = "1">
        </div>
</div>
<div class="form-group row">
        <label for="degreeType" class="col-2 col-form-label">Degree Type</label>
        <div class="col-10">
            <input type="text" name="degreeType" value="<%=items[0].degreeType%>" class="form-control" maxlength = "255">
        </div>
</div>
<div class="form-group row">
        <label for="overallRating" class="col-2 col-form-label">Overall Rating</label>
        <div class="col-10">
            <input type="text" name="overallRating" value="<%=items[0].overallRating%>" class="form-control" maxlength = "255" readonly>
        </div>
</div>
<div class="form-group row">
        <label for="submit" class="col-2 col-form-label" style="color:#ffffff">Submit</label>
        <div class="col-10">
            <input type="submit" name="submit" value="Save Program" class="btn btn-primary btn-lg">
        </div>
</div>

</form>


</div>
<% include ../partials/footer %>