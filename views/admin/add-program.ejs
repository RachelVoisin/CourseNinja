<% include ../partials/header %>
<% include ../partials/admin-header %>
<div class="row" style="margin-left: 50px; margin-right: 50px">


<form action="/admin/program/add" method="post" class="col s12">

<div class="form-group row">
	<label for="schoolName" class="col-2 col-form-label">School Name</label>
	<div class="col-10">
		<input type="text" id="schoolInput" name="schoolName" value="" class="form-control" maxlength="255">
	</div>
</div>

<div class="form-group row">
    <label for="programCode" class="col-2 col-form-label">Program Code</label>
    <div class="col-10">
        <input type="text" name="programCode" value="" class="form-control" maxlength = "255" required>
    </div>
</div>
<div class="form-group row">
        <label for="programName" class="col-2 col-form-label">Program Name</label>
        <div class="col-10">
            <input type="text" name="programName" value="" class="form-control" maxlength = "255" required>
        </div>
</div>
<div class="form-group row">
        <label for="programLength" class="col-2 col-form-label">Program Length</label>
        <div class="col-10">
            <input type="text" name="programLength" value="" class="form-control" maxlength = "255">
        </div>
</div>
<div class="form-group row">
        <label for="tuitionDomestic" class="col-2 col-form-label">Domestic Tuition</label>
        <div class="col-10">
            <input type="number" name="tuitionDomestic" value="" class="form-control" maxlength = "10">
        </div>
</div>
<div class="form-group row">
        <label for="tuitionInternational" class="col-2 col-form-label">International Tuition</label>
        <div class="col-10">
            <input type="number" name="tuitionInternational" value="" class="form-control" maxlength = "10">
        </div>
</div>
<div class="form-group row">
        <label for="isCoOp" class="col-2 col-form-label">Is Co-Op (1 or 0)</label>
        <div class="col-10">
            <input type="number" name="isCoOp" value="" class="form-control" min="0" max="1" maxlength = "1">
        </div>
</div>
<div class="form-group row">
        <label for="degreeType" class="col-2 col-form-label">Degree Type</label>
        <div class="col-10">
            <input type="text" name="degreeType" value="" class="form-control" maxlength = "255">
        </div>
</div>
<div class="form-group row">
        <label for="submit" class="col-2 col-form-label" style="color:#ffffff">Submit</label>
        <div class="col-10">
            <input type="submit" name="submit" value="Save Program" class="btn btn-primary btn-lg">
        </div>
</div>

</form>


</div>

<script src="/scripts/autocomplete.js"></script>
<script>
var url = "/autocomplete";

$(document).ready(function(){
	var data = {"school": document.getElementById("schoolInput").value, "search": "school"};

	$.ajax({
		type: "GET",
		url: url,
		data: data,
		success: function(result){ 
			autocomplete(document.getElementById("schoolInput"), result);
		},
		error:function (jXHR, textStatus, errorThrown){
			console.log(errorThrown);
			console.log(textStatus);
			console.log(jXHR);
		},
		dataType: "JSON"
	});
});
</script>

<% include ../partials/footer %>